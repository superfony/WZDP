<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/supply_manage"
    tools:context=".MovedActivity"
    tools:ignore="HardcodedText" >

    <com.epsmart.wzcc.view.CircleLayoutTwo
        android:id="@+id/daily_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center_horizontal" >

        <com.epsmart.wzcc.view.CircleImageView
            android:id="@+id/pronotice_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/daily_notice_btn" />

        <com.epsmart.wzcc.view.CircleImageView
            android:id="@+id/prowork_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/daily_work_btn" />

        <com.epsmart.wzcc.view.CircleImageView
            android:id="@+id/proonline_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/daily_online_btn" />
    </com.epsmart.wzcc.view.CircleLayoutTwo>

</RelativeLayout><!-- From: file:/E:/as/WZDP/wZDP/src/main/res/layout/activity_daily.xml -->