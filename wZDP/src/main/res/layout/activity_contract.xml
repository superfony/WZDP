<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/supply_manage"
    tools:ignore="HardcodedText"
    tools:context=".ContractActivity" >

	 <com.epsmart.wzcc.view.CircleLayoutTwo
        android:id="@+id/contract_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center_horizontal"

        >
               
<!--         circle:circleBackground="@drawable/green"  > -->

     <com.epsmart.wzcc.view.CircleImageView
            android:id="@+id/plan_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_allplan_btn" />

        <com.epsmart.wzcc.view.CircleImageView
            android:id="@+id/progress_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_allprogress_btn" />

    </com.epsmart.wzcc.view.CircleLayoutTwo>



</RelativeLayout>